{
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": true,
    "endOfLine": "lf",
    "useTabs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "css",
    "singleAttributePerLine": false,
    "embeddedLanguageFormatting": "auto",
    "experimentalTernaries": true,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "insertSpaceBeforeFunctionParenthesis": true
}